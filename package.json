{
    "name": "text-format",
    "displayName": "Text Format",
    "description": "Edit text, Edit variable type...",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.95.0"
    },
    "categories": [
        "Keymaps",
        "Formatters",
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "my-extension.helloWorld",
                "title": "Hello World Command"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "npm run copy-files && webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "lint": "eslint src",
        "test": "vscode-test",
        "copy-files": "copyfiles -u 1 node_modules/free-google-translate-api-ts/**/* ./dist/"
    },
    "dependencies": {
        "free-google-translate-api-ts": "^1.0.1"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.9",
        "@types/node": "20.x",
        "@types/vscode": "^1.95.0",
        "@typescript-eslint/eslint-plugin": "^8.10.0",
        "@typescript-eslint/parser": "^8.7.0",
        "eslint": "^9.13.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.6.3",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4"
    }
}
